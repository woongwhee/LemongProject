<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="jdbcTypeForNull" value="NULL"/>
    </settings>
    <typeAliases>
        <typeAlias type="site.lemongproject.web.template.model.dto.Template" alias="Template"/>
        <typeAlias type="site.lemongproject.web.template.model.dto.TemplateCategory" alias="TemplateCategory"/>
        <typeAlias type="site.lemongproject.web.template.model.dto.TemplateTodo" alias="TemplateTodo"/>
        <typeAlias type="site.lemongproject.web.template.model.vo.TemplateUpdateVo" alias="TemplateUpdateVo"/>
        <typeAlias type="site.lemongproject.web.member.model.vo.Profile" alias="Profile"/>
        <typeAlias type="site.lemongproject.web.member.model.vo.Member" alias="Member"/>
        <typeAlias type="site.lemongproject.web.photo.model.vo.Photo" alias="Photo"/>
        <typeAlias type="site.lemongproject.web.follow.model.vo.Follow" alias="Follow"/>
        <typeAlias type="site.lemongproject.web.reply.model.vo.Reply" alias="Reply"/>
        <typeAlias type="site.lemongproject.common.domain.dto.OfficialHoliday" alias="Holiday"/>
        <typeAlias type="site.lemongproject.web.feed.model.vo.Feed" alias="Feed"/>
    </typeAliases>

    <mappers>
        <mapper resource="mybatis/mappers/member-mapper.xml"/>
        <mapper resource="mybatis/mappers/follow-mapper.xml"/>
        <mapper resource="mybatis/mappers/reply-mapper.xml"/>
        <mapper resource="mybatis/mappers/profile-mapper.xml"/>
        <mapper resource="mybatis/mappers/template-mapper.xml"/>
        <mapper resource="mybatis/mappers/templatetodo-mapper.xml"/>
        <mapper resource="mybatis/mappers/templatecategory-mapper.xml"/>
        <mapper resource="mybatis/mappers/holiday-mapper.xml"/>
        <mapper resource="mybatis/mappers/feed-mapper.xml"/>
        <mapper resource="mybatis/mappers/photo-mapper.xml"/>
    </mappers>
</configuration>
